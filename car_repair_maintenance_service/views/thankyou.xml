    <template id="successful_feedback_car" name="Success Page">
        <t t-call="website.layout">
            <div id="wrap">
                <br/><br/>
                <div class="oe_structure"/>
                <div class="container">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="alert alert-info">
                                <strong> Your Feedback have been sent successfully.<br/>
                                    Thank you,<br/>
                                    Repair Team
                                </strong>
                            </div>
                            <a class="btn btn-primary pull-left mb32" 
                            onclick="history.go(-1);" >
                                <span class="hidden-xs">Go Back</span>
                                <span class="fa fa-long-arrow-left"/>
                            </a>
                        </div>
                        <div class="col-md-6">
                            <img width="200" height="200" 
                            src="/car_repair_maintenance_service/static/description/success.png" 
                            class="img-square" />
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>
    
