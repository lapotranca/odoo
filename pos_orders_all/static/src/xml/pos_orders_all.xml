			<div class="control-button button confirm" id="pos_bag" style="background-color: #7f82ac; color: #fff;font-size: 14px;">
				<i class="fa fa-shopping-bag"></i> Bag 
			</div>
		</t>
	</t>

	<t t-name="GiftButtonWidget" name="POS Coupens Gift Voucher">
		 <div class="control-button button confirm" style="background-color: #7f82ac;color: #fff; font-size: 14px;">
				<i class="fa fa-gift"></i>Coupon
		  </div>
	</t>

	<!-- POSBarcodeReturnWidget -->
	<t t-name="POSBarcodeReturnWidget" name="POS Return Order with Barcode">
		<div class="control-button button confirm" id="create_barcode_return" style="background-color: #7f82ac; color: #fff;font-size: 14px;">
			<i class="fa fa-barcode"></i> Return with Barcode
		</div>
	</t>


	<!-- PosBarcodePopupWidget -->
	<t t-name="PosBarcodePopupWidget">
